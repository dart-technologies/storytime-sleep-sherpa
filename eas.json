{
    "cli": {
        "version": ">= 5.8.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",
            "env": {
                "APP_VARIANT": "development",
                "EXPO_BUNDLE_IDENTIFIER": "art.dart.storytime.dev",
                "EXPO_ANDROID_PACKAGE": "art.dart.storytime.dev"
            }
        },
        "development-simulator": {
            "extends": "development",
            "ios": {
                "simulator": true
            }
        },
        "preview": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "preview",
            "env": {
                "APP_VARIANT": "preview",
                "EXPO_BUNDLE_IDENTIFIER": "art.dart.storytime.preview",
                "EXPO_ANDROID_PACKAGE": "art.dart.storytime.preview"
            }
        },
        "production": {
            "distribution": "store",
            "autoIncrement": true,
            "channel": "production",
            "env": {
                "APP_VARIANT": "production",
                "EXPO_BUNDLE_IDENTIFIER": "art.dart.storytime",
                "EXPO_ANDROID_PACKAGE": "art.dart.storytime"
            }
        }
    }
}